<Window x:Class="SabNzbdUploader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="336" Width="525" WindowStyle="None" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid Loaded="Grid_Loaded">
        <ListBox Height="132" HorizontalAlignment="Left" Margin="12,95,0,0" Name="CategorySelector" VerticalAlignment="Top" Width="479" />
        <Label Content="Please select a category for this NZB" Height="33" HorizontalAlignment="Left" Margin="12,56,0,0" Name="label1" VerticalAlignment="Top" Width="479" FontStretch="Normal" FontFamily="Arial" FontSize="20" />
        <Button Content="Upload" Height="44" HorizontalAlignment="Right" Margin="0,241,12,0" Name="UploadButton" VerticalAlignment="Top" Width="140" FontSize="20" FontFamily="Arial" Click="UploadButton_Click" />
        <Label Content="Label" Height="38" HorizontalAlignment="Left" Margin="12,12,0,0" Name="FilenameLabel" VerticalAlignment="Top" Width="479" FontFamily="Arial" FontSize="20" />
    </Grid>
</Window>
